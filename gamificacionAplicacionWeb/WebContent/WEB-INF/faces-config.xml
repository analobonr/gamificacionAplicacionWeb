<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>beans.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>RegistroBean</managed-bean-name>
		<managed-bean-class>beans.RegistroBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>configurarPartida</managed-bean-name>
		<managed-bean-class>beans.configurarPartida</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>partidasBean</managed-bean-name>
		<managed-bean-class>beans.PartidasBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gestionUsuariosBean</managed-bean-name>
		<managed-bean-class>beans.gestionUsuariosBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gestionJuegosBean</managed-bean-name>
		<managed-bean-class>beans.gestionJuegosBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>
